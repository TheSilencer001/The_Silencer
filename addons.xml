<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.losmovies" version="2.8" name="Los Movies" provider-name="The_Silencer">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.urlresolver"  version="1.0.0"/>
        <import addon="script.module.addon.common" version="2.0.0"/>
	<import addon="script.module.metahandler"  version="1.0.0"/>
   	<import addon="script.common.plugin.cache" version="2.5.5"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Browse losmovies.com</summary>
        <description>Play a wide selection of movies, tv-shows and HD content from www.losmovies.com</description>
        <platform>all</platform>
    </extension>
</addon>

